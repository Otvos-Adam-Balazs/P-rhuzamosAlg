package MultiBead;

public class Multithreading {

	public static void main(String[] args) {
		
		long t1,t2,d;
		t1 = System.currentTimeMillis();
		for(int i = 0; i <4 ;i++) {
			MultithreadThing myThing = new MultithreadThing(i);
			myThing.start();
		}
		t2 = System.currentTimeMillis();
		d = t2 - t1; //eltelt ido
	}

}
